/* This exercise is to print odd values from a name, in this case a string.*/

#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define max 100

int main() {
  char name[max];
  int i,length;
  printf("Type single name without space with 100 letters at maximum: ");
  gets(name);
  length=strlen(name);

  for(i=1; i<length; i+=2){
    printf("%c \n",name[i]);
  }

  return 0;
}
